{
  "name": "aio-cli-plugin-telemetry",
  "description": "Adobe I/O cli telemetry",
  "version": "0.1.1-next.0",
  "author": "Adobe Inc.",
  "bugs": "https://github.com/adobe/aio-cli-plugin-telemetry/issues",
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "debug": "^4.1.1",
    "insight": "^0.10.3",
    "splunk-logging": "^0.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/plugin-help": "^5.1.11",
    "eslint": "^8.8.0",
    "eslint-config-oclif": "^3.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/adobe/aio-cli-plugin-telemetry",
  "keywords": [
    "ecosystem:@adobe/aio-cli-plugin",
    "oclif-plugin"
  ],
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./src/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./src/hooks/init"
      ],
      "prerun": [
        "./src/hooks/prerun"
      ],
      "postrun": [
        "./src/hooks/postrun"
      ],
      "command_not_found": [
        "./src/hooks/command-not-found"
      ],
      "telemetry": [
        "./src/hooks/telemetry"
      ]
    }
  },
  "repository": "adobe/aio-cli-plugin-telemetry",
  "scripts": {
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint src test e2e",
    "test": "jest --ci && npm run lint"
  }
}
